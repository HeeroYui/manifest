<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote name="origin"
	        fetch="https://github.com/"/>
	<default revision="master" remote="origin" sync-s="true"/>
	<!--
	    name : repository name
	    path : installation path of the repository
	-->
	<project name="HeeroYui/etk"         path="etk"/>
	<project name="HeeroYui/ejson"       path="ejson"/>
	<project name="HeeroYui/exml"        path="exml"/>
	<project name="HeeroYui/esvg"        path="esvg"/>
	<project name="HeeroYui/enet"        path="enet"/>
	<project name="HeeroYui/ege"         path="ege"/>
	<project name="HeeroYui/egami"       path="egami"/>
	<project name="HeeroYui/date"        path="date"/>
	<project name="HeeroYui/ewol"        path="ewol"/>
	<project name="HeeroYui/audio-ess"   path="audio-ess"/>
	
	<!-- generic library -->
	<project name="HeeroYui/libfreetype" path="lib/freetype"/>
	<project name="HeeroYui/z"           path="lib/z"/>
	<project name="HeeroYui/libagg"      path="lib/agg"/>
	<project name="HeeroYui/gtest"       path="lib/gtest"/>
	<project name="HeeroYui/glew"        path="lib/glew"/>
	<project name="HeeroYui/bullet"      path="lib/bullet"/>
	<project name="HeeroYui/libpng"      path="lib/png"/>
	<project name="HeeroYui/edtaa3"      path="lib/edtaa3"/>
	
	<!-- tools -->
	<project name="HeeroYui/ios-deploy"  path="tools/ios-deploy"/>
	
	<!-- came from : https://github.com/musicdsp/manifest.git : default.xml -->
	<include name="musicdsp.xml"/>
</manifest>